<main class="main">
  <div class="inner">
      <% if current_user %>
    <%# ログインしているときは以下を表示する %>
       <div class="greeting">
        こんにちは、
      <%= link_to current_user.name, user_path(current_user.id), class: :greeting__link%>
      さん
       </div>
           <% end %> 
    <%# // ログインしているときは上記を表示する %>
   <div class="card__wrapper">
    <% @prototypes.each do |prototype| %>
     <%= render partial: "prototype", locals: { prototype: prototype } %>
     <% end %>
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    </div>
  </div>
</main>
